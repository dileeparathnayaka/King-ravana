function successfullMessage(msg) {
    return "🇱🇰 *king ravana*:  ```" + msg + "```"
}
function errorMessage(msg) {
    return "🥴 *kingravana*:  ```" + msg + "```"
}
function infoMessage(msg) {
    return "🐲 *king ravana*:  ```" + msg + "```"
}


module.exports = {
    successfullMessage,
    errorMessage,
    infoMessage
}
